<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.pw.edu.mini.zpoif.Application.HelloController">

    <Label fx:id="welcomeText" text="Cos tam tytul aplikacji" />
    <TabPane prefHeight="700.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE" >
        <tabs>
            <Tab fx:id="start_button" text="start">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    </AnchorPane>
                </content>
            </Tab>
            <Tab fx:id="tabela_button" text="tabela">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="500.0">
                        <TableView fx:id="table" prefHeight="700.0" prefWidth="900.0">
                            <columns>
                                <TableColumn fx:id="currency" prefWidth="75.0" style="-fx-alignment: CENTER;" text="currency" />
                                <TableColumn fx:id="date" prefWidth="75.0" style="-fx-alignment: CENTER;" text="date" />
                                <TableColumn fx:id="rate" prefWidth="75.0" style="-fx-alignment: CENTER;" text="rate" />
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </TableView>
                    </AnchorPane>
                </content>
            </Tab>
            <Tab fx:id="mapa_button" text="mapa">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    </AnchorPane>
                </content>
            </Tab>
            <Tab text="cos tam">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                </content>
            </Tab>
        </tabs>
    </TabPane>
</ScrollPane>
